{
  "version": 2,
  "builds": [
    {
      "src": "boa-connect/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "boa-connect/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd boa-connect && npm install && npm run build",
  "outputDirectory": "boa-connect/dist"
}
